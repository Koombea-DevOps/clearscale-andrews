[
  {
    "memoryReservation": 256,
    "name": "${container_name}",
    "mountPoints": [],
    "image": "${docker_image}",
    "essential": true,
    "cpu": 0,
    "volumesFrom": [],
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80,
        "hostPort": 0
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${project_name}-${environment}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${project_name}"
      }
    }
  }
]
